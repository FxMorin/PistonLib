{
  "schemaVersion": 1,
  "id": "pistonlib",
  "version": "${version}",
  "name": "PistonLib",
  "description": "A mod/library that expands the piston system",
  "authors": [
    "FX - PR0CESS",
    "Space Walker"
  ],
  "contributors": [
    "Shnupbups"
  ],

  "license": "MIT",
  "icon": "assets/pistonlib/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "ca.fxco.pistonlib.PistonLib"
    ],
    "client": [
      "ca.fxco.pistonlib.PistonLibClient"
    ],
    "fabric-datagen": [
      "ca.fxco.pistonlib.datagen.PistonLibDatagen"
    ],
    "pistonlib": [
      "ca.fxco.pistonlib.PistonLib"
    ],
    "preLaunch": [
      "com.llamalad7.mixinextras.MixinExtrasBootstrap::init"
    ]
  },
  "accessWidener" : "pistonlib.accesswidener",
  "mixins": [
    "pistonlib.mixins.json"
  ],
  "depends": {
    "minecraft": "1.19.3",
    "fabricloader": ">=0.14.0",
    "fabric": ">=0.71.0",
    "java": ">=17"
  },
  "custom": {
    "loom:injected_interfaces": {
      "net/minecraft/class_4970": [
        "ca/fxco/api/pistonlib/block/PLBlockBehaviour"
      ],
      "net/minecraft/class_4970\u0024class_4971": [
        "ca/fxco/api/pistonlib/block/state/PLBlockStateBase"
      ],
      "net/minecraft/class_4970\u0024class_2251": [
        "ca/fxco/api/pistonlib/block/PLBlockProperties"
      ],
      "net/minecraft/class_2586": [
        "ca/fxco/api/pistonlib/blockEntity/PLBlockEntity"
      ],
      "net/minecraft/class_1792": [
        "ca/fxco/api/pistonlib/item/PLItem"
      ],
      "net/minecraft/class_1792\u0024class_1793": [
        "ca/fxco/api/pistonlib/item/PLItemProperties"
      ],
      "net/minecraft/class_1937": [
        "ca/fxco/api/pistonlib/level/PLLevel"
      ]
    },
    "modmenu": {
      "links": {
        "modmenu.discord": "https://discord.gg/mezndgtD83"
      }
    }
  }
}
