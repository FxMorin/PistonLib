{
  "schemaVersion": 1,
  "id": "pistonlib",
  "version": "${version}",
  "name": "PistonLib",
  "description": "A mod/library that expands the piston system",
  "authors": [
    "FX - PR0CESS",
    "Space Walker"
  ],
  "contributors": [
    "Shnupbups"
  ],

  "license": "MIT",
  "icon": "assets/pistonlib/icon.png",

  "environment": "*",
  "entrypoints": {
    "main": [
      "ca.fxco.pistonlib.PistonLib"
    ],
    "client": [
      "ca.fxco.pistonlib.PistonLibClient"
    ],
    "fabric-datagen": [
      "ca.fxco.pistonlib.datagen.PistonLibDatagen"
    ],
    "pistonlib": [
      "ca.fxco.pistonlib.PistonLib"
    ],
    "preLaunch": [
      "com.llamalad7.mixinextras.MixinExtrasBootstrap::init"
    ],
    "fabric-gametest": [
      "ca.fxco.pistonlib.gametest.BasicTestSuite"
    ]
  },
  "accessWidener" : "pistonlib.accesswidener",
  "mixins": [
    "pistonlib.mixins.json"
  ],
  "depends": {
    "minecraft": "1.19.3",
    "fabricloader": ">=0.14.0",
    "fabric": ">=0.71.0",
    "java": ">=17"
  },
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.discord": "https://discord.gg/mezndgtD83"
      }
    }
  }
}
